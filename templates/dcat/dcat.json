      "id": "dcat:Distribution",
      "constraints": {
        "http://www.w3.org/1999/02/22-rdf-syntax-ns#type": "http://www.w3.org/ns/dcat#Distribution"
      },
      "label": {},
      "styles": [],
      "items": [
        {
          "id": "dcat:dcterms:title"
        },
        {
          "id": "dcat:dcterms:description"
        },
        {
          "id": "dcat:dcterms:issued"
        },
